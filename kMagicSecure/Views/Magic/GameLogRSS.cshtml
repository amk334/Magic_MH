@model IEnumerable<Magic.Domain.GameLogEntry>
@{
    ViewBag.Title = "View";
}

@{
	Layout = null;	
}

<rss xmlns:blogChannel="https://kmagic.azurewebsites.net/Magic/GameLogRSS" version="2.0">
	<channel>
		<title> TITLE		</title>
		<link> LINK </link>
		<description> DESC </description>
		<language>en-gb</language>
		@foreach (var item in Model)
		{
			<item>
				<description> @item.Description </description>
				<category> @item.EventName </category>
				<author> @item.User </author>
			</item>
		}
	</channel>
</rss>
